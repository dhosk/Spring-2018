    "#boxscoreDF = pd.DataFrame(columns=[\"GAME_ID\",\"SEASON\",\"SERIES_ID\",\"GAME_NUM\",\"TEAM_ID\",\"TEAM_ABBREVIATION\",\"TEAM_CITY\",\"PLAYER_ID\",\"PLAYER_NAME\",\"START_POSITION\",\"COMMENT\",\"MIN\",\"OFF_RATING\",\"DEF_RATING\",\"NET_RATING\",\"AST_PCT\",\"AST_TOV\",\"AST_RATIO\",\"OREB_PCT\",\"DREB_PCT\",\"REB_PCT\",\"TM_TOV_PCT\",\"EFG_PCT\",\"TS_PCT\",\"USG_PCT\",\"PACE\",\"PIE\"])\n",
    "\n",
    "## get all teams\n",
    "for i in range(80,96):\n",
    "    yearStr = str(i)\n",
    "    yearEnd = str(i+1)\n",
    "    if i//10==0:\n",
    "        yearStr = '0'+ yearStr\n",
    "    \n",
    "    if (i+1)//10==0:\n",
    "        yearEnd = '0'+yearEnd\n",
    "    \n",
    "    season = '19'+ yearStr +'-'+yearEnd\n",
    "#    season = '1999-00'\n",
    "    print(season)\n",
    "    params = {'LeagueID':'00','Season':season,'IsOnlyCurrentSeason':'0'}\n",
    "    teams = get_nba_data('commonTeamYears', params)\n",
    "    ## get all players\n",
    "    #params = {'LeagueID':'00', 'Season': '2017-18', 'IsOnlyCurrentSeason': '0',\n",
    "    #         'StartPeriod':'0','EndPeriod':'10','StartRange':'10','EndRange':'28800',\n",
    "    #         'RangeType':'0','GameID':GameID}\n",
    "    playoffGames = get_nba_data('commonplayoffseries', params)\n",
    "\n",
    "    #boxscoreDF = pd.DataFrame(columns=colnames)\n",
    "    for GameID in playoffGames.GAME_ID:\n",
    "        params = {'LeagueID':'00', 'Season': season, 'IsOnlyCurrentSeason': '0',\n",
    "                  'StartPeriod':'0','EndPeriod':'10',\n",
    "                  'StartRange':'10','EndRange':'28800',\n",
    "                  'RangeType':'0','GameID':GameID}\n",
    "        boxscore = get_nba_data('boxscoretraditionalv2',params)\n",
    "        boxscore['SEASON']=season\n",
    "        boxscore['SERIES_ID'] = playoffGames.SERIES_ID                 \n",
    "        boxscoreDF = pd.concat([boxscoreDF,boxscore],ignore_index=True)\n",
    "    print(boxscore.head())\n",
    "    #playoffGames\n",
    "boxscoreDF"
